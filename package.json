{
  "name": "blog",
  "version": "0.0.1",
  "description": "my blog build with vuepress",
  "main": "index.js",
  "authors": {
    "name": "shufo",
    "email": "meikyowise@gmail.com"
  },
  "repository": "shufo/shufo.dev",
  "scripts": {
    "dev": "vuepress dev docs",
    "build": "vuepress build docs",
    "serve": "vuepress serve docs",
    "lint": "eslint --ext .js,.vue .",
    "fix": "eslint --fix --ext .js,.vue ."
  },
  "license": "MIT",
  "devDependencies": {
    "@goy/vuepress-plugin-svg-icons": "^4.2.1",
    "@vuepress/plugin-google-analytics": "^1.6.0",
    "@vuepress/theme-blog": "^2.3.2",
    "cache-loader": "1.2.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.0.1",
    "husky": "^4.3.0",
    "imagemin-lint-staged": "^0.4.0",
    "lint-staged": "^10.4.0",
    "vuepress": "^1.6.0",
    "vuepress-plugin-autometa": "^0.1.13",
    "vuepress-plugin-autonav": "^0.1.3",
    "vuepress-plugin-code-copy": "^1.0.6",
    "vuepress-plugin-feed": "^0.1.8",
    "vuepress-plugin-seo": "^0.1.3",
    "vuepress-plugin-serve": "^2.0.4",
    "vuepress-plugin-sitemap": "^2.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn exec lint-staged"
    }
  },
  "lint-staged": {
    "resources/**/*.{js,jsx,vue}": [
      "eslint --fix",
      "git add"
    ],
    "resources/**/*.{css,scss}": [
      "stylelint --fix",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif,svg}": [
      "imagemin-lint-staged",
      "git add"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.20"
  }
}
